{
    "parser": "babel-eslint",
    "rules": {
        "strict": 0,
        "semi": 2,
        "comma-dangle": [2, "never"],
        "no-cond-assign": 2,
        "no-constant-condition": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-semi": 2,
        "no-irregular-whitespace": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "eqeqeq": 2,
        "no-undef": 2,
        "no-unused-vars": 2,

        "react/jsx-uses-react": 1,
        "react/react-in-jsx-scope": 1,
        "react/no-unknown-property": 1
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "es6": true
    }
}

